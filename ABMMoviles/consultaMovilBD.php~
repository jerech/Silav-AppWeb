<?php
	include('conexionBD.php');
	
	$conectar = establecerConexion();
	$id=$_POST["id"];
    $sql = "SELECT patente FROM Moviles where numero=".$id;
    $select = mysql_query($sql,$conectar);
    
    if($select){
    	$resultado = mysql_fetch_row($select);
    	echo $resultado[0];
    	mysql_close($conectar);
    }else {
    	mysql_close($conectar);
    }
    
 ?>